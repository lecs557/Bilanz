<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane prefHeight="203.0" prefWidth="372.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.windows.NewAccountWindowCtrl">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="143.0" prefWidth="228.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="278.0">
               <children>
                  <Label contentDisplay="TOP" text="Name">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="tb_name" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="37.0" prefWidth="278.0">
               <children>
                  <Label contentDisplay="TOP" text="IBAN">
                     <HBox.margin>
                        <Insets right="24.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="tb_iban" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="47.0" prefHeight="37.0" prefWidth="278.0">
               <children>
                  <Label contentDisplay="TOP" text="BIC">
                     <HBox.margin>
                        <Insets right="33.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="tb_bic" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER_RIGHT" layoutX="14.0" layoutY="164.0" prefHeight="25.0" prefWidth="359.0">
         <children>
            <Button fx:id="btn_ok" mnemonicParsing="false" onAction="#ok" prefWidth="52.0" text="OK">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btn_close" mnemonicParsing="false" onAction="#close" prefWidth="52.0" text="Close">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</Pane>
