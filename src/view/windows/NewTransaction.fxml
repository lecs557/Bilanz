<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="203.0" prefWidth="372.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.windows.NewTransactionCtrl">
    <children>
      <Pane fx:id="enterTransactionContainer" prefHeight="120.0" prefWidth="300.0" />
        <HBox alignment="CENTER_RIGHT" layoutX="14.0" layoutY="164.0" prefHeight="25.0" prefWidth="359.0">
            <children>
                <Button fx:id="btn_ok" mnemonicParsing="false" onAction="#ok" prefWidth="52.0" text="OK">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="btn_close" mnemonicParsing="false" onAction="#close" prefWidth="52.0" text="Close">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</Pane>
